{
  "name": "@awiejacha/homiecountry",
  "version": "1.0.3",
  "main": "dist/country.min.js",
  "types": "dist/country.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Adrian Wiejacha",
  "license": "UNLICENSED",
  "description": "Homie Country utils",
  "private": false,
  "scripts": {
    "transpile": "rm -rf dist && tsc",
    "minify": "find dist -type f -name \"*.min.js\" -delete && find dist -type f -name \"*.js\" ! -name \"*.min.js\" -exec sh -c 'npx esbuild \"$1\" --platform=node --target=es2022 --minify --outfile=\"${1%.js}.min.js\"' _ {} \\;",
    "build": "npm run transpile && npm run minify",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "lint": "npx eslint . --ext .ts",
    "test": "[ -d \"node_modules/\" ] || npm ci && npx jest",
    "knip": "knip"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "knip": "^5.85.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
